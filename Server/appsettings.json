{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=PurchaseNexus;User Id=;Trusted_Connection=True;MultipleActiveResultSets=true",
    "AuthDbContextConnection": "Server=(localdb)\\mssqllocaldb;Database=PurchaseNexus.Server;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Information",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      },
      "Enrich": [
        "FromLogContext"
      ],
      "WriteTo": [
        {
          "Name": "Console"
        }
      ]
    }
  },
  "AllowedHosts": "*"
}